<?xml version="1.0" encoding="utf-8"?>
<package xmlns="https://raw.githubusercontent.com/NuGet/NuGet.Client/dev/src/NuGet.Core/NuGet.Packaging/compiler/resources/nuspec.xsd">
    <metadata>
        <!-- Required elements-->
        <id>DaanV2.Essentials.Net</id>
        <version>1.4.2</version>
        <description> This library provides a set of default interfaces and code extensions upon .Net to ease up programming.</description>
        <authors>DaanVerstraten</authors>

        <!-- Optional elements -->
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <license type="file">LICENSE.txt</license>
        <projectUrl>https://github.com/DaanV2/DaanV2.Essentials.Net</projectUrl>
        <copyright>Daan Verstraten 2019</copyright>
        <tags>essentials interfaces code-extensions</tags>
        <repository type="git" url="https://github.com/DaanV2/DaanV2.Essentials.Net" />
        <icon>images\icon.png</icon>
        <readme>docs\readme.md</readme>
        <releaseNotes>Fixing package</releaseNotes>

        <dependencies>
            <group targetFramework=".NETCore5.0" />
        </dependencies>

        <!-- ... -->
    </metadata>
    <!-- Optional 'files' node -->
    <files>
        <file src="..\README.md" target="docs\" />  
        <file src="..\Nuget\images\icon.png" target="images\" />
        <file src="..\LICENSE.txt" target="" />

        <!-- Any CPU -->
        <file src="lib\net5.0\*.*" target="lib\net5.0\" exclude="*.tmp;*.pdb"/>
    </files>
</package>